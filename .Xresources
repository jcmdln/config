! ~/.Xresources

xft.antialias: true
xft.autohint: false
xft.dpi: 96
xft.hinting: true
xft.hintstyle: hintfull
xft.lcdfilter: lcddefault
xft.rgba: rgb

xterm.allowScrollLock: true
xterm.buffered: true
xterm.bufferedFPS: 60
xterm.termName: xterm-256color
xterm.toolBar: false
xterm.vt100.allowPasteControls: true
xterm.vt100.altIsNotMeta: true
xterm.vt100.altSendsEscape: true
xterm.vt100.alternateScroll: true
xterm.vt100.background: black
xterm.vt100.deleteIsDEL: false
xterm.vt100.faceName: Noto Mono,Noto Emoji
xterm.vt100.faceSize1: 6
xterm.vt100.faceSize2: 10
xterm.vt100.faceSize3: 14
xterm.vt100.faceSize4: 18
xterm.vt100.faceSize5: 22
xterm.vt100.faceSize6: 26
xterm.vt100.faceSize: 10
xterm.vt100.fastScroll: true
xterm.vt100.foreground: grey
xterm.vt100.locale: false
xterm.vt100.loginShell: true
xterm.vt100.multiScroll: true
xterm.vt100.renderFont: true
xterm.vt100.saveLines: 16384
xterm.vt100.scrollBar: false
xterm.vt100.scrollKey: true
xterm.vt100.scrollTtyOutput: false
xterm.vt100.selectToClipboard: true
xterm.vt100.trimSelection: true
xterm.vt100.utf8: 1
xterm.vt100.utf8Fonts: 1
xterm.vt100.utf8Title: 1

xterm.vt100.translations: #override \n\
    Ctrl <Key> +: larger-vt-font() \n\
    Ctrl <Key> -: smaller-vt-font() \n\
    Ctrl <Key> 0: set-vt-font(default) \n\
    Ctrl Shift <Key>C: copy-selection(SELECT) \n\
    Ctrl Shift <Key>V: insert-selection(SELECT)
